<%= javascript_include_tag Ckeditor.cdn_url %>

<div class="page-container">
  <div class=" col-1-3 category-list-container --test">
    <p class="notice"><b><%= notice %></b></p>
    <p class="alert"><b><%= alert %></b></p>
    <%= render @categories %>
  </div>

  <div class="col-1-3 featured-article-container">
    <h3>Featured Article!</h3>
    <%= render '/articles/featured_article', locals: {featured_article: @featured_article} %>
  </div>
</div>

