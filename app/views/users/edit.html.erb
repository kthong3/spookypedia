<%= javascript_include_tag Ckeditor.cdn_url %>
<div class="page-container">
<div class="user-views">
<% if logged_in? && authorized?(@user) %>
  <%= form_for @user, html: {multipart: true} do |f|  %>
   <p>
     <%= f.label :image %><br>
     <%= f.file_field :image %>
   </p>
   <p>
     <%= f.submit %>
   </p>
  <% end %>
<% end %>
</div>
</div>